{
  "langDetector": {
    "fromHtmlTag": false,
    "regexp": "^(?:/.+)?/.+/([a-z]{2,3})/.+",
    "default": "wel"
  },
  "mods": {
    "header": {
      "languages": {
        "wel": "cy"
      },
      "showPortalSwitcher": "",
      "topCustomMenu": [
        "gn-site-name-menu",
        "gn-portal-switcher",
        "gn-search-menu",
        "gn-map-menu",
        "gn-contribute-menu",
        "gn-admin-menu",
        "ogc-api-welsh",
        "english-portal"
      ]
    },
    "home": {
      "appUrl": "../../cym/wel/catalog.search#/home",
      "showMosaic": false,
      "showSocialBarInFooter": false
    },
    "search": {
      "appUrl": "../../cym/wel/catalog.search#/search",
      "savedSelection": {
        "enabled": true
      },
      "usersearches": {
        "enabled": true,
        "includePortals": true,
        "displayFeaturedSearchesPanel": false
      }
    },
    "editor": {
      "enabled": false
    },
    "admin": {
      "enabled": false
    },
    "authentication": {
      "enabled": false
    },
    "map": {
      "enabled": false
    },
    "footer": {
      "showSocialBarInFooter": false
    },
    "recordview": {
      "isSocialbarEnabled": false
    }
  },
  "baseURLDetector": {
    "regexp": "^((?:/.+)?)+/.+/[a-z]{2,3}/.+",
    "default": "/geonetwork"
  },
  "nodeDetector": {
    "regexp": "^(?:/.+)?/(.+)/[a-z]{2,3}/.+",
    "default": "srv"
  }
}