{
  "langDetector": {
    "fromHtmlTag": false,
    "regexp": "^(?:/.+)?/.+/([a-z]{2,3})/.+",
    "default": "eng"
  },
  "nodeDetector": {
    "regexp": "^(?:/.+)?/(.+)/[a-z]{2,3}/.+",
    "default": "srv"
  },
  "serviceDetector": {
    "regexp": "^(?:/.+)?/.+/[a-z]{2,3}/(.+)",
    "default": "catalog.search"
  },
  "baseURLDetector": {
    "regexp": "^((?:/.+)?)+/.+/[a-z]{2,3}/.+",
    "default": "/geonetwork"
  },
